{
    "id": "publish",
    "name": "Eidos Publish",
    "description": "Make doc available to the public",
    "version": "0.0.7",
    "main": "dist/main.js",
    "commands": [
        {
            "name": "publish",
            "description": "publish a doc",
            "inputJSONSchema": {
                "type": "object",
                "properties": {
                    "slug": {
                        "type": "string"
                    }
                },
                "required": []
            },
            "outputJSONSchema": {
                "type": "object",
                "properties": {
                    "url": {
                        "type": "string"
                    }
                },
                "required": [
                    "url"
                ]
            },
            "asTableAction": true
        },
        {
            "name": "unpublish",
            "description": "unpublish a doc",
            "inputJSONSchema": {
                "type": "object",
                "properties": {},
                "required": []
            },
            "asTableAction": true
        },
        {
            "name": "register",
            "description": "register a subdomain",
            "inputJSONSchema": {
                "type": "object",
                "properties": {
                    "subdomain": {
                        "type": "string"
                    }
                },
                "required": [
                    "subdomain"
                ]
            }
        }
    ],
    "tables": [],
    "envs": [
        {
            "name": "SUBDOMAIN",
            "type": "string",
            "readonly": true
        },
        {
            "name": "TOKEN",
            "type": "string",
            "readonly": true
        }
    ]
}